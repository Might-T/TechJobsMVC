<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{

    // TODO #3 - Use the same list display from Views/Search/Index.cshtml
    @if (ViewBag.jobs != null)
    {



        <table class="job-listing">
            @foreach (Dictionary<string, string> jobPost in ViewBag.jobs)
            {
                <tr>
                    @foreach (KeyValuePair<string, string> jobDetail in jobPost)
                    {
                        <td>@jobDetail.Key:@jobDetail.Value </td>
                    }
                </tr>
            }
        </table>
